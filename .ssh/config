Host github.com-anonymous-fly
   HostName github.com
   User git
   IdentityFile ~/.ssh/anon-github
   IdentitiesOnly yes

Host *.aci.ics.psu.edu
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h-%p
    ControlPersist 600
    User suv87
    RequestTTY yes
    # RemoteCommand /usr/bin/fish
    RemoteCommand zsh
    # RemoteCommand /storage/home/suv87/work/linuxbrew/.linuxbrew/bin/zsh

